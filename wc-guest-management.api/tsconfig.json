{
    "compilerOptions": {
        "target": "es5",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "outDir": "./build",
        "sourceMap": true,
        "baseUrl": "src",
        "downlevelIteration": true
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/**/*.spec.ts"
    ]
}